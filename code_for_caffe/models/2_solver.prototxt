net: "./2_train_test.prototxt"
test_iter: 128
test_interval: 128
# lr for fine-tuning should be lower than when starting from scratch
base_lr: 0.01
lr_policy: "step"
gamma: 0.1
# stepsize should also be lower, as we're closer to being done
stepsize: 1000
display: 20
max_iter: 50000
momentum: 0.9
weight_decay: 0.0001
snapshot: 10000
snapshot_prefix: "./2_fnt"
# uncomment the following to default to CPU mode solving
# solver_mode: CPU
